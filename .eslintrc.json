{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "semi": "off",
        "no-console": "off",
        "lines-around-comment": "off",
        "max-len": "off",
        "camelcase": "off",
        "eqeqeq": "off",
        "dot-notation": "off",
        "no-useless-escape": "off",
        "no-undef": "off",
        "space-in-parens": "off",
        "quotes": "off",
        "key-spacing": "off"
    }
}