{
  "type": "thinx_server_config",
  "transmit_key": "<Global transmit key for device environment submissions using API>",
  "version" : "1",
  "public_url": "<enter-your-console-fqdn>",
  "api_url": "<enter-your-api-fqdn>",
  "mailgun": {
    "api_key": "<mailgun-api-key>",
    "base_url": "<mailgun-base-url>",
    "domain": "<mailgun-domain>"
  },
  "redis": {
    "host": "thinx-redis",
    "db": 0,
    "port": 6379
  },
  "mqtt": {
    "server": "mqtt://mosquitto",
    "port": 1883
  },
  "mqtt_port": 1883,
  "port": 7442,
  "secure_port": 7443,
  "lambda": 7475,
  "data_root": "/mnt/data",
  "deploy_root": "/data",
  "build_root": "/repos",
  "ssh_keys": "/mnt/data/ssh_keys",
  "slack" : {
    "client_id" : "",
    "client_secret": "",
    "webhook": "",
    "bot_token": "",
    "bot_topic": "thinx"
  },
  "ssl_key": "/mnt/data/ssl/privkey.pem",
  "ssl_cert": "/mnt/data/ssl/cert.pem",
  "client_user_agent": "THiNX-Client",
  "builder" : {
    "concurrency" : 2
  },
  "debug": {
    "device" : false,
    "deployment": false,
    "allow_http_login": true,
    "ingress": false,
    "secrets": false
  },
  "strict_gdpr": true
}
