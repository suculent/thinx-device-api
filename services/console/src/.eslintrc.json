{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": ["jquery", "eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "moment": true,
        "angular": true,
        "Thinx": true,
        "Morris":true,
        "Layout": true,
        "QuickSidebar": true,
        "Theme": true,
        "getTimezones": true,
        "CryptoJS": true,
        "toastr": true,
        "App": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "lines-around-comment": "off",
        "max-len": "off",
        "camelcase": "off",
        "eqeqeq": "off",
        "dot-notation": "off",
        "no-useless-escape": "off",
        "quotes": ["single", "always"]
    }
}
