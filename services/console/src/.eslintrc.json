{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": ["jquery", "eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "moment": true,
        "angular": true,
        "Thinx": true,
        "Morris":true,
        "Layout": true,
        "QuickSidebar": true,
        "Theme": true,
        "getTimezones": true,
        "CryptoJS": true,
        "toastr": true,
        "App": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "quotes": "off",
        "linebreak-style": "off",
        "space-in-parens": "off",
        "computed-property-spacing": "off",
        "space-before-function-paren": "off",
        "lines-around-comment": "off",
        "object-curly-spacing": "off",
        "array-bracket-spacing": "off",
        "no-trailing-spaces": "off",
        "max-len": "off",
        "camelcase": "off",
        "eqeqeq": "off",
        "dot-notation": "off"
    }
}
