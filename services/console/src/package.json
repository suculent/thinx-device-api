{
  "name": "thinx-console",
  "version": "1.0.0",
  "description": "THiNX Console",
  "main": "gulpfile.js",
  "scripts": {
    "test": "protractor test/conf.js",
    "coverage": "npm test; nyc report --reporter=text-lcov | coveralls",
    "build": "./node_modules/.bin/gulp build",
    "buildAll": "./node_modules/.bin/gulp buildPublic buildApp",
    "lint": "./node_modules/.bin/eslint --ignore-path .eslintignore ./",
    "preinstall": "npx npm-force-resolutions"
  },
  "author": "Mikulas Sychra @ THiNX Cloud",
  "repository": {
    "type": "git",
    "url": "git://github.com/suculent/thinx-console.git"
  },
  "license": "ISC",
  "dependencies": {
    "minimatch": "3.0.4",
    "negotiator": "0.6.3",
    "qs": "6.10.3"
  },
  "overrides": {
    "graceful-fs": "^4.2.9",
    "negotiator": "0.6.3"
  },
  "resolutions": {
    "graceful-fs": "^4.2.9",
    "negotiator": "0.6.3"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^8.7.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-jquery": "^3.0.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-jquery": "^1.5.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-debug": "^4.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-inject-envs": "^1.1.1",
    "gulp-noop": "^1.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-remove-code": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.4.2",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^3.0.0",
    "gulp-util": "^3.0.8",
    "merge-stream": "^2.0.0",
    "npm-force-resolutions": "^0.0.10",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "html/app/**/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
